## JFROG ##
# JFROG DATABASE
JFROG_USER=jfrog
JFROG_DB=jfrog
# Make sure you set this to a unique secure random value on production
JFROG_PASSWORD=jfrog # If you change this, the dashboard import will fail.  You will need to update the password in the dashboard zip files.
DB_DRIVER="postgresql+psycopg2"
JF_TOKEN=<TOKEN>
JF_URL=<JF_URL>

LOG_LEVEL=INFO
## JFROG ##

JFROG_UPDATE_SHORT_INTERVAL=1
JFROG_UPDATE_LONG_INTERVAL=30



# database configurations (do not modify)
JFROG_DATABASE_HOST=jfrog_db
DATABASE_PORT=5432
DATABASE_DIALECT=postgresql


REDIS_HOST=jfrog_redis
REDIS_PORT=6379